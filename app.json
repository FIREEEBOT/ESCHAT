{
    "name": "EsChatBot",
    "description": "Hello, I am Es An Intelligent ChatBot. If You Are Feeling Lonely, You can Always Come to me and Chat With Me!",
    "logo": "https://telegra.ph/file/6732bb149b330e4b4f241.jpg",
    "repository": "https://github.com/FIREEEBOT/ESCHAT",
    "env": {
        "TOKEN": {
            "description": "telegram bot token here",
            "required": true
        },
        "KUKI_KEY": {
            "description": "Contact https://t.me/kukiaisupport for free Api Key.",
            "required": true
        },
        "API_ID": {
            "description": "telegram api id here",
            "required": true
        },
        "API_HASH": {
            "description": "telegram api hash here",
            "required": true
        }
    },
    "addons": [
        {
            "plan": "heroku-postgresql"
        }
    ]
}
